@model List<parkSmartly.Data.Model.Space>

@{
    ViewBag.Title = "List my spaces";
    Layout = "~/Views/Shared/_LayoutMaterialAdmin.cshtml";
    ViewBag.PageName = "myspace";
    ViewBag.MenuPropertyOwnerClass = "active";

}
<div class="card col-md-12">
    <div class="row">
        <div class="col-md-12">
            <div class="card-head">
                <header>My carpark</header>
            </div>
            <!--end .card-head -->
            <div class="card-body">

                <div class="row">
                    <div class="col-lg-12">
                        <div class="row">
                            <div class="col-lg-6">
                                <a href="@Url.Action("Index","PostSpace")" class="btn btn-primary" type="button" id="cf-submit">Post a new space</a>
                            </div>
                        </div>
                        <div id="contact" class="form-container">
                            <fieldset>
                                <table class="table">
                                    <thead>
                                        <tr>
                                            <th>Address</th>
                                            <th>Status</th>
                                        </tr>

                                    </thead>
                                    @foreach (parkSmartly.Data.Model.Space space in Model)
                                    {
                                        <tr>
                                            <td>@space.Address</td>
                                            <td>Pending</td>
                                        </tr>
                                    }
                                </table>
                            </fieldset>
                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>